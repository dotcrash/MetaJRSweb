.g-ft {
  background-color: #23222a;
  color: #fff;
  padding: 20rpx 35rpx;
}
.g-ft1 {
  margin-bottom: 40rpx;
}
.i-contact {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
}
.i-contact .img-qcode {
  width: 160rpx;
  margin-right: 20rpx;
}
.i-contact .img-qcode image {
  width: 160rpx;
  height: 160rpx;
}
.i-contact .img-qcode text {
  width: 160rpx;
  display: inline-block;
  line-height: 60rpx;
  font-size: 28rpx;
  text-align: center;
  font-weight: 700;
}
.i-contact .u-tel {
  padding: 30rpx;
}
.i-contact .u-tel .tit {
  font-size: 30rpx;
  font-weight: 700;
}
.i-contact .u-tel .num {
  font-size: 30rpx;
  text-indent: 2em;
  line-height: 60rpx;
  font-weight: 700;
}
.g-ft2 {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.g-ft2 .u-text {
  line-height: 60rpx;
}
.gongan {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.gongan image {
  margin-top: 5rpx;
  margin-right: 10rpx;
  width: 30rpx;
  height: 30rpx;
}
.gongan .p {
  font-size: 28rpx;
  line-height: 40rpx;
}

